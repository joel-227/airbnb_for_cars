<div class="container" style="width: 400px; margin: 10px 0;">
  <h1>List A Car</h1>
</div>
<div class="container" style="width: 400px; margin: 10px 0;" > 
  <%= simple_form_for @car do |f| %>
    <%= f.input :price %>
    <%= f.input :model %>
    <%= f.input :license %>
    <%= f.input :age %>
    <%= f.button :submit, class:"btn btn-primary"%>
  <% end %>
</div>
