<h1>My Cars</h1>

<hr>
<% @cars.each do |car| %>
  <p><%= link_to "#{car.model}", my_car_path(car) %> | <%= link_to 'edit', edit_car_path(car) %> | <%= link_to 'delete', car_path(car), method: :delete, data: {confirm: "This will delete #{car.model}!"} %></p>
<% end %>

