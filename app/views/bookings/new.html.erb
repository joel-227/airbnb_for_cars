<div id='car-booking-dates' data-unavailable="<%= @car.unavailable_dates.to_json %>">
    <% if current_user != @car.user  %>
       <%= simple_form_for([@car, @booking]) do |f| %>
           <%= f.input :start_time, as: :string, type: :text %>
           <%= f.input :end_time, as: :string, type: :text %>
           <%= f.submit "Book", class: "btn btn-primary" %>
        <% end %>
    <% end %>
 </div>